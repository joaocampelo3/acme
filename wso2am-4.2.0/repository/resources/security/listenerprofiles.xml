<parameter xmlns:svns="http://org.wso2.securevault/configuration" name="SSLProfiles">
    <profile>
        <bindAddress>0.0.0.0</bindAddress>
         <KeyStore>
            <Location>repository/resources/security/wso2carbon.jks</Location>
            <Type>JKS</Type>
            <Password>wso2carbon</Password>
            <KeyPassword>wso2carbon</KeyPassword>
        </KeyStore>
        <TrustStore>
            <Location>repository\resources\security\client-truststore-temp.jks</Location>
            <Type>JKS</Type>
            <Password>wso2carbon</Password>
        </TrustStore>
        <SSLVerifyClient>optional</SSLVerifyClient>
        <HttpsProtocols>TLSv1,TLSv1.1,TLSv1.2</HttpsProtocols>
    </profile>
</parameter>